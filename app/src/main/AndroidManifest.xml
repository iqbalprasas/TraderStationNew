<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.telc2.traderstation">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_ts"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_ts"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name="com.example.telc2.traderstation.MainActivity" />
        <activity
            android:name="com.example.telc2.traderstation.activity.LoginActivity"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.telc2.traderstation.activity.MenuActivity">

        </activity>
        <activity android:name=".activity.ProfileActivity"/>
        <activity android:name=".activity.SearchResultActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
            <meta-data android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity android:name=".activity.ChattingActivity"/>
        <activity android:name=".activity.TraderPerformanceActvity"/>
        <activity android:name=".activity.WebPageActivity"/>
        <activity android:name=".activity.WebinarActivity"/>
        <activity android:name=".activity.WebinarSelectedActivity"/>
        <activity android:name=".activity.CalendarActivity"/>
        <activity android:name=".activity.ECSelectedActivity"/>
        <activity android:name=".activity.CompetitionActivity"/>
        <activity android:name=".activity.EventActivity"/>
        <activity android:name=".activity.RebateBrokerActivity"/>
        <activity android:name=".activity.BrokerActivity"/>
        <activity android:name=".activity.ReviewBrokerActivity"/>
        <activity android:name=".activity.EAPerformanceActivity"/>
        <activity android:name=".activity.PammActivity"/>
        <activity android:name=".activity.PammSelectedActivity"/>
        <activity android:name=".activity.GalleryActivity"/>
        <activity android:name=".activity.AlbumSelectedActivity"/>
        <activity android:name=".activity.EventSelectedActivity"/>
        <activity android:name=".activity.TimelineSelectedActivity"/>
    </application>

</manifest>